<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="60">
  <Appenders>
	<Console name="Console" target="SYSTEM_OUT">
	  <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %c{1.}:%method:%line [%-5level] - %msg%n"/>
	</Console>
  </Appenders>
  <Loggers>
	<Root level="debug">
	  <AppenderRef ref="Console"/>
	</Root>
  </Loggers>
</Configuration>
